{
  "name": "written-number",
  "version": "0.1.0",
  "description": "Convert numbers to their written form.",
  "main": "lib/index.js",
  "scripts": {
    "test": " mocha --require blanket -R mocha-spec-cov-alt"
  },
  "keywords": [
    "written",
    "number",
    "english",
    "int",
    "word",
    "human",
    "spoken"
  ],
  "author": "Pedro Tacla Yamada",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamadapc/js-written-number"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "mocha": "^2.2",
    "mocha-spec-cov-alt": "^0.3.0",
    "should": "^6.0.3"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules"
      ],
      "pattern": [
        "lib"
      ],
      "spec-cov": {
        "threshold": 80,
        "localThreshold": 80,
        "lcovOutput": "lcov"
      }
    }
  }
}
